<?xml version="1.0" encoding="UTF-8"?>
<api context="/email" name="SendEmail" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET">
        <inSequence>
            <log/>
            <send>
                <endpoint key="email"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
